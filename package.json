{
  "name": "daisyui",
  "version": "0.2.5",
  "description": "CSS components for Tailwind CSS",
  "author": "Pouya Saadeghi",
  "license": "MIT",
  "homepage": "https://github.com/saadeghi/daisyui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saadeghi/daisyui.git"
  },
  "bugs": {
    "url": "https://github.com/saadeghi/daisyui/issues"
  },
  "keywords": [
    "design-system",
    "tailwindcss",
    "components",
    "ui-library",
    "component",
    "framework",
    "tailwind",
    "theming",
    "postcss",
    "design",
    "css",
    "ui"
  ],
  "main": "base.js",
  "typings": "base.js",
  "files": [
    "dist",
    "preset",
    "base.js",
    "styled.js"
  ],
  "publishConfig": {
    "access": "public",
    "branches": [
      "master"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": [],
      "tailwindcss": "preset/index.js",
      "postcss-nested": {
        "bubble": [
          "screen"
        ]
      },
      "autoprefixer": [],
      "cssnano": {
        "preset": [
          "default",
          {
            "mergeRules": false
          }
        ]
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.0.4",
    "cssnano": "^4.1.10",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.1",
    "prejss-cli": "^0.3.3",
    "tailwindcss": "^2.0.1"
  },
  "peerDependencies": {
    "tailwindcss": "^2.0.0"
  },
  "scripts": {
    "postcss": "postcss base/index.css -o dist/base.css && postcss styled/index.css -o dist/styled.css",
    "json": "prejss-cli dist/base.css --format commonjs && prejss-cli dist/styled.css --format commonjs",
    "build": "npm run postcss && npm run json",
    "add": "touch base/$npm_config_name.css && touch styled/$npm_config_name.css && echo '@import \"./'$npm_config_name'\";' >> base/index.css && echo '@import \"./'$npm_config_name'\";' >> styled/index.css && open base/$npm_config_name.css && open styled/$npm_config_name.css"
  }
}
